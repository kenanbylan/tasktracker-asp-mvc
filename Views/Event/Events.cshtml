@using Microsoft.AspNetCore.Identity
@using Azure.Core
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager
@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Mvc


@{
    ViewData["Title"] = "Tüm Aktivitelerimize Göz Atın";
    var defaultImage = "data:image/jpeg;base64,";
    @model List<tasktracker.Models.Event>

    
    }



@*bread crumbs container start*@
<div class="m-3">
    <div class="row bg-body-tertiary rounded-3">
        <nav class="col-8" aria-label="breadcrumb">
            <ol class="breadcrumb breadcrumb-chevron p-4 my-auto">
                <li class="breadcrumb-item">
                    <a class="link-body-emphasis" asp-controller="Home" asp-action="Index">
                        <i class="bi bi-house" style="font-size: 1.1rem;"></i>
                        <span class="visually-hidden">Home</span>
                    </a>
                </li>
                <li class="breadcrumb-item">
                    <a class="link-body-emphasis fw-semibold text-decoration-none" asp-controller="Event" asp-action="Events">Etkinlikler</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                    Tüm Etkinlikler
                </li>
            </ol>
            @if (!string.IsNullOrEmpty(Context.Request.Query["searchText"]))
            {
                <div class="alert alert-info" role="alert">
                    Arama Sonuçları: <strong>@Context.Request.Query["searchText"]</strong>
                </div>
            }

        </nav>

        <form class="m-3 col" role="search" method="get" action="@Url.Action("SearchEvents", "Event")">
            <input type="search" class="form-control" placeholder="Ara..." aria-label="Search" name="searchText">
        </form>


    </div>
</div>

@*Main-content-start*@

<div class="album py-4 bg-body-tertiary rounded-3">
    <div class="container">
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">



            @foreach (var e in Model)
            {
                @if (!string.IsNullOrEmpty(Context.Request.Query["searchText"]))
                {
                    <div class="col">
                        <div class="card shadow-sm">
                            @if (e.event_image != "[]")
                            {
                                <img class="bd-placeholder-img card-img-top" width="100%" height="225" src="@e.event_image" alt="Aktivite Resmi">
                            }
                            else
                            {
                                <img class="bd-placeholder-img card-img-top" width="100%" height="225" src="@defaultImage" alt="Aktivite Resmi">
                            }
                            <div class="card-body">
                                <p class="card-text" style="font-size: larger; font-weight: bold; color: darkred;">@e.event_name</p>
                                <p class="card-text">
                                    @if (e.event_description.Length > 100)
                                    {
                                        <span>@e.event_description.Substring(0, 100)</span>
                                        <span id="more-@e.event_id" style="display: none;">@e.event_description.Substring(100)</span>
                                    }
                                    else
                                    {
                                        <span>@e.event_description</span>
                                    }
                                </p>
                                <div class="d-flex justify-content-between align-items-center">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-sm btn-outline-secondary">
                                            <a class="text-decoration-none text-secondary" asp-controller="Event" asp-action="EventView" asp-route-id="@e.event_id">Görüntüle</a>
                                        </button>
                                        <button type="button" class="btn btn-sm btn-outline-info">
                                            <a href="#" class="btn text-decoration-none btn-sm text-primary" onclick="toggleDescription(@e.event_id)">Dahası...</a>
                                        </button>

                                        @if (SignInManager.IsSignedIn(User))
                                        {
                                            <form method="post" asp-action="JoinEvent" >
                                                <input type="hidden" name="eventId" value="@e.event_id" />
                                                <button type="submit" class="btn btn-sm btn-outline-secondary">
                                                    <span class="text-decoration-none text-secondary">Etkinliğe Katıl</span>
                                                </button>
                                            </form>
    
                                        }
                                    </div>
                                    <small class="text-body-secondary">@e.event_date</small>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <div class="col">
                        <div class="card shadow-sm">

                            @if (e.event_image != "[]")
                            {
                                <img class="bd-placeholder-img card-img-top" width="100%" height="225" src="@e.event_image" alt="Aktivite Resmi">
                            }
                            else
                            {
                                <img class="bd-placeholder-img card-img-top" width="100%" height="225" src="@defaultImage" alt="Aktivite Resmi">
                            }

                            <div class="card-body">
                                <p class="card-text" style="font-size: larger; font-weight: bold; color: darkred;">@e.event_name</p>
                                <p class="card-text">
                                    @if (e.event_description.Length > 100)
                                    {
                                        <span>@e.event_description.Substring(0, 100)</span>
                                        <span id="more-@e.event_id" style="display: none;">@e.event_description.Substring(100)</span>
                                    }
                                    else
                                    {
                                        <span>@e.event_description</span>
                                    }
                                </p>
                                <div class="d-flex justify-content-between align-items-center">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-sm btn-outline-secondary">
                                            <a class="text-decoration-none text-secondary" asp-controller="Event" asp-action="EventView" asp-route-id="@e.event_id">Görüntüle</a>
                                        </button>
                                        
                                        <button type="button" class="btn btn-sm btn-outline-info">
                                            <a href="#" class="btn text-decoration-none btn-sm text-primary" onclick="toggleDescription(@e.event_id)">Dahası...</a>
                                        </button>

                                        @if (SignInManager.IsSignedIn(User))
                                        {
                                            
                                            <form method="post" asp-action="JoinEvent" >
                                               <input type="hidden" name="eventId" value="@e.event_id" />
                                               <button type="submit" class="btn btn-sm btn-outline-secondary">
                                                   <span class="text-decoration-none text-secondary">Etkinliğe Katıl</span>
                                               </button>
                                           </form>
                                            
                                        }
                                    </div>
                                    <small class="text-body-secondary">@e.event_date</small>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>

        @*Pagination-buttons*@
        <div class="row m-2 justify-content-center">
            <nav aria-label="Page navigation example">
                <ul class="pagination justify-content-center">
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                        </a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#">1</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#">2</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#">3</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
</div>

<script>
    function toggleDescription(eventId) {
        var moreDescription = document.getElementById("more-" + eventId);
        var button = event.target;
        
        if (moreDescription.style.display === "none") {
            moreDescription.style.display = "inline";
            button.textContent = "Daha Az Göster";
            button.classList.remove("text-primary");
            button.classList.add("text-danger");
        } else {
            moreDescription.style.display = "none";
            button.textContent = "Daha Fazla Göster";
            button.classList.remove("text-danger");
            button.classList.add("text-primary");
        }
    }
</script>